# Supabase 配置
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here
SUPABASE_JWT_SECRET=your_supabase_jwt_secret_here

# JWT 配置（用于 session token）
JWT_SECRET_KEY=your-secret-key-change-in-production
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=43200  # 30 天

# 阿里云百炼配置（可选，优先使用用户提供的 API Key）
BAILIAN_API_KEY=
BAILIAN_APP_ID=your_app_id_here
BAILIAN_MODEL_NAME=qwen-max

# 阿里云 OSS 配置
OSS_REGION=oss-cn-shanghai
OSS_ACCESS_KEY_ID=your_access_key_id_here
OSS_ACCESS_KEY_SECRET=your_access_key_secret_here
OSS_BUCKET=your_bucket_name_here

# CORS 允许的源（多个用逗号分隔）
CORS_ORIGINS=http://localhost:5173,http://localhost:3000,http://localhost

# 服务端口
PORT=8000
